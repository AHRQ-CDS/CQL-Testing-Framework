    "eslint": "^7.26.0",
    "fs-extra": "^7.0.1",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.21",
    "mocha": "^8.4.0",
    "semver": "^5.6.0",
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "cql-execution": ">=1.3.0"
  },
  "devDependencies": {
    "cql-execution": "^2.2.0"
  },
  "resolutions": {
    "lodash": "^4.17.21"
